<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/View_constraint_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:context=".view.ChallengeFragment">

            <!-- 메뉴바 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout1"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@drawable/bg_item_line"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/pretendard_medium"
                    android:text="소비 유형"
                    android:textColor="@color/gray_500"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout1">

                <ImageButton
                    android:id="@+id/btn1"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginStart="45dp"
                    android:layout_marginTop="45dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/btn2"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginTop="45dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/btn3"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginTop="45dp"
                    android:layout_marginEnd="45dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/btn4"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginStart="45dp"
                    android:layout_marginTop="50dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn1" />

                <ImageButton
                    android:id="@+id/btn5"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginTop="50dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn2" />

                <ImageButton
                    android:id="@+id/btn6"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginTop="50dp"
                    android:layout_marginEnd="45dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn3" />

                <ImageButton
                    android:id="@+id/btn7"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginStart="45dp"
                    android:layout_marginTop="50dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn4" />

                <ImageButton
                    android:id="@+id/btn8"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginTop="50dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn5" />

                <ImageButton
                    android:id="@+id/btn9"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginTop="50dp"
                    android:layout_marginEnd="45dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn6" />

                <ImageButton
                    android:id="@+id/btn10"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginStart="45dp"
                    android:layout_marginTop="50dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn7" />

                <ImageButton
                    android:id="@+id/btn11"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_marginTop="50dp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_mypage_ex"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn8" />

                <ImageView
                    android:layout_width="322dp"
                    android:layout_height="167dp"
                    android:layout_marginTop="57dp"
                    android:layout_marginBottom="60dp"
                    android:src="@color/orange_100"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn11" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</layout>